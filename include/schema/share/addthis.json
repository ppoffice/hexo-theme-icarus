{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "/share/addthis.json",
    "description": "Enable AddThis share buttons\nhttps://www.addthis.com/",
    "type": "object",
    "properties": {
        "type": {
            "type": "string",
            "const": "addthis"
        },
        "install_url": {
            "type": "string",
            "description": "URL to the AddThis share plugin script"
        }
    },
    "required": [
        "type",
        "install_url"
    ]
}